<% if @avv %>
  <% variant = @avv.variant %>
  <% product = variant.product %>
  <% url = variant.is_master? ? vendor_product_path(@vendor, product) : vendor_product_path(@vendor, product)  %>
  <%# no variant pages set up yet for customer %>
  <%# url = variant.is_master? ? vendor_product_path(@vendor, product) : vendor_product_variant_path(@vendor, product, variant) %>

  $('#update-price-<%= @avv.id %>').replaceWith("<%= j(render(partial: 'spree/cust/products/variant_price.html.erb', locals: { avv: @avv, variant: variant, url: url })) %>");
  $('#variant_qty_<%= variant.id %>').prop('disabled', false);

<% end %>
