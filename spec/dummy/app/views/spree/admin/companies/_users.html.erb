<div class='panel-body' id='users-container'>
  <table class='table'>
    <thead>
      <tr>
        <th>
          First
        </th>
        <th>
          Last
        </th>
        <th>
          Email
        </th>
        <th>
          Last Sign In
        </th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td>
            <%= user.firstname %>
          </td>
          <td>
            <%= user.lastname %>
          </td>
          <td>
            <%= link_to user.email, edit_admin_user_path(user) %>
          </td>
          <td>
            <%= user.last_sign_in_at %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
